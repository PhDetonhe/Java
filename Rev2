import java.util.Scanner;
public class rev002 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		double s = 0;

		for (int i = 0; i < 2; i++) {
			System.out.print("Informe o " + (i + 1) + "ยบ valor: ");
			double v = sc.nextDouble();
			s += v;
		}
		System.out.print("Resultado da soma: " + s);
	}
}
