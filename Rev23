public class rev023 {
	public static void main(String[] args) {
		int[] v = {1, 4, 19, 27, 33, 39, 50};
		int max = v[0];

		System.out.println("Valores no vetor:");
		for (int i = 0; i < v.length; i++) {
			System.out.println(v[i]);
			if (v[i] > max) {
				max = v[i];
			}
		}
		System.out.println("Maior valor: " + max);
	}
}
