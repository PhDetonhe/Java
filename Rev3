import java.util.Scanner;
public class rev003 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		double s = 0, m;

		for (int i = 0; i < 4; i++) {
			System.out.print("Digite a " + (i + 1) + "ª nota: ");
			double n = sc.nextDouble();
			s += n;
		}
		m = s / 4;
		System.out.print("A média final é: " + m);
	}
}
